# Supabase - Get from https://app.supabase.com/project/YOUR_PROJECT/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# JWT Secret - Generate a random 32+ character string
JWT_SECRET=your_jwt_secret_min_32_characters

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Midtrans - Get from https://dashboard.midtrans.com
MIDTRANS_SERVER_KEY=your_midtrans_server_key
MIDTRANS_CLIENT_KEY=your_midtrans_client_key
NEXT_PUBLIC_MIDTRANS_CLIENT_KEY=your_midtrans_client_key
MIDTRANS_IS_PRODUCTION=false
NEXT_PUBLIC_MIDTRANS_IS_PRODUCTION=false

# Kolosal AI - Get from https://api.kolosal.ai
KOLOSAL_API_KEY=your_kolosal_api_key

# Google Cloud AI (Vertex AI dengan Gemini)
# Option 1: Set credentials as base64 JSON
GEMINI_CREDENTIALS=your_base64_encoded_service_account_json
# Option 2: Set path to service account JSON file (for local development)
# GOOGLE_APPLICATION_CREDENTIALS=./gemini-credentials.json

# Gemini Project Configuration
GEMINI_PROJECT_ID=your_gcp_project_id
GEMINI_LOCATION=us-central1
